{
  "endOfLine": "auto",
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "es5",
  "semi": true,
  "newline-before-return": true,
  "no-duplicate-variable": [true, "check-parameters"],
  "no-var-keyword": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/constants/(.*)$",
    "^@/context/(.*)$",
    "^@/lib/(.*)$",
    "^[./]",
    "^(.*).css",
    "^(.*).(svg|jpg|png)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "pluginSearchDirs": false,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ]
}
